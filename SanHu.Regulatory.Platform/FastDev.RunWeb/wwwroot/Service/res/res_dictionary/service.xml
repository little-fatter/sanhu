<?xml version="1.0" encoding="utf-8"?>
<freedesign xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
    <model name="res_dictionary" title="字典" textField="Title" moduleName="res" moduleTitle="基础资料" />
    <fields>
        <field name="Title" dbName="Title" title="标题" type="string" length="100" />
        <field name="DicCode" dbName="DicCode" title="代码标示" type="string" length="100" />
        <field name="Remarks" dbName="Remarks" title="备注" type="string" length="2000" />
        <field name="Items" title="明细项" type="one2many" relationModel="res_dictionaryItems" relationField="DicID" />
    </fields>
    <create base_fields="*" base_ignore_fields="" many2one="*" one2many="" many2many="" />
    <update base_fields="*" base_ignore_fields="" many2one="*" one2many="" many2many="" />
    <delete base_fields="*" base_ignore_fields="" many2one="*" one2many="" many2many="" />
    <getdetail base_fields="*" base_ignore_fields="" many2one="*" one2many="" many2many="" />
    <getlist base_fields="*" base_ignore_fields="" many2one="" one2many="" many2many="" />
    <getpageddata base_fields="*" base_ignore_fields="" many2one="" one2many="" many2many="" />
</freedesign>
