<?xml version="1.0" encoding="utf-8"?>
<freedesign xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
    <model name="eventInfo" title="事件" textField="Title" moduleName="case" moduleTitle="案件" dbName="" />
    <fields>
        <field name="Title" dbName="Title" title="标题" type="string" length="50" isPK="N" />
        <field name="Address" dbName="Address" title="地址" type="string" length="200" isPK="N" />
        <field name="Addressx" dbName="Addressx" title="地址x" type="string" length="50" isPK="N" />
        <field name="Addressy" dbName="Addressy" title="地址y" type="string" length="50" isPK="N" />
        <field name="Reportingtime" dbName="Reportingtime" title="上报时间" type="string" length="50" isPK="N" />
        <field name="NameOfReporter" dbName="NameOfReporter" title="上报人名称" type="string" length="50" isPK="N" />
        <field name="PhoneOfReporter" dbName="PhoneOfReporter" title="上报人手机" type="string" length="50" isPK="N" />
        <field name="EventType" dbName="EventType" title="事件类型" type="string" length="50" isPK="N" />
        <field name="Describe" dbName="Describe" title="描述" type="string" length="500" relationModel="" relationField="" isPK="N" />
        <field name="EventState" dbName="EventStateID" title="事件状态" type="many2one" relationModel="res_dictionaryItems" relationField="" isPK="N" />
        <field name="ProcessingResult" dbName="ProcessingResult" title="处理结果" type="string" length="50" isPK="N" />
        <field name="NeedLawEnforcement" dbName="NeedLawEnforcement" title="需要执法" type="boolean" relationModel="" relationField="" isPK="N" />
        <field name="NeedTracking" dbName="NeedTracking" title="需要跟踪" type="string" length="50" isPK="N" />
    </fields>
    <create base_fields="*" base_ignore_fields="" many2one="*" one2many="" many2many="" />
    <update base_fields="*" base_ignore_fields="" many2one="*" one2many="" many2many="" />
    <delete base_fields="*" base_ignore_fields="" many2one="*" one2many="" many2many="" />
    <getdetail base_fields="*" base_ignore_fields="" many2one="*" one2many="" many2many="" />
    <getlist base_fields="*" base_ignore_fields="" many2one="EventState" one2many="" many2many="" />
    <getpageddata base_fields="*" base_ignore_fields="" many2one="EventState" one2many="" many2many="" />
</freedesign>